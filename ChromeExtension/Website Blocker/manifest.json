{
	"name": "Website Blocker",
	"version": "1.0",
	"description": "A extension to block websites",
	"permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
	"background": {
		"scripts": ["blocker.js"]
	},
	"manifest_version": 2,
	"icons": {
		"128": "icons/128.png",
		"48": "icons/48.png",
		"16": "icons/16.png"
	}
}
